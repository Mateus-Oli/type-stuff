{
  "env": {
    "es6": true
  },
  "settings": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "quotes": "off",
    "eqeqeq": "error",
    "max-depth": ["error", { "max": 3 }],
    "semi": "error",
    "no-var": "error",
    "no-cond-assign": "off",
    "no-sparse-arrays": "off"
  },
  "globals": {
    "console": true
  }
}
